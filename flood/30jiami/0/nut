FoamFile
{
    version         2;
    format          ascii;
    class           volScalarField;
    location        "5399.9";
    object          nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -1 0 0 0 0 ];

internalField   uniform 0.001;

boundaryField
{
    Inflow
    {
        type            zeroGradient;
    }
    Outflow
    {
        type            zeroGradient;
    }
    Boundary0
    {
        type            externalCoupled;
        commsDir        "$FOAM_CASE/comms";
        file            data;
        calcFrequency   600;
        timeOut         1000;
        initByExternal  yes;
        log             no;
    }
    Boundary
    {
        type            zeroGradient;
    }
    TopAndBot
    {
        type            empty;
    }
}


